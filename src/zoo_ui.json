{
    "components": [
        {
            "label": "modelOptions",
            "key": "modelOptions",
            "type": "hidden",
            "input": true,
            "tableView": false
        },
        {
            "label": "actionOptions",
            "key": "actionOptions",
            "type": "hidden",
            "input": true,
            "tableView": false
        },
        {
            "label": "Select model",
            "widget": "choicesjs",
            "tableView": true,
            "dataSrc": "custom",
            "data": {
                "custom": "values = data.modelOptions"
            },
            "validate": {
                "required": true
            },
            "key": "selectModel",
            "properties": {
                "triggerHappy": "modelSelectionChanged"
            },
            "type": "select",
            "input": true
        },
        {
            "label": "Select action",
            "widget": "choicesjs",
            "tableView": true,
            "dataSrc": "custom",
            "data": {
                "custom": "values = data.actionOptions"
            },
            "validate": {
                "required": true
            },
            "key": "selectAction",
            "properties": {
                "triggerHappy": "actionSelectionChanged"
            },
            "conditional": {
                "show": true,
                "when": "actionOptions"
            },
            "customConditional": "console.log(data); show = data.actionOptions.length > 0;",
            "type": "select",
            "input": true
        },
        {
            "label": "Model Settings",
            "className": "simian.gui.composed_component.PropertyEditor",
            "displayHeight": 100,
            "tableView": false,
            "key": "settings",
            "type": "customcomposed",
            "input": true
        },
        {
            "label": "Initialize and run model",
            "showValidations": true,
            "disableOnInvalid": true,
            "tableView": false,
            "key": "runButton",
            "type": "button",
            "event": "runModel",
            "action": "event",
            "input": true
        }
    ]
}